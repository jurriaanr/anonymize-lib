    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Jurriaan Ruitenberg",
            "email": "jur@oberon.nl"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "doctrine/annotations": "^1.8",
        "doctrine/common": "^2.11",
